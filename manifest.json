{
  "manifest_version": 3,
  "name": "DietGuard",
  "version": "1.0",
  "description": "Search for Food Restrictions on the current web page.",
  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}


    
